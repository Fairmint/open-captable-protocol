    "solidity.packageDefaultDependenciesContractsDirectory": "src",
    "solidity.formatter": "prettier",
    "solidity.compileUsingRemoteVersion": "v0.8.20",
    "solidity.remappings": [
        "diamond-3-hardhat/=lib/diamond-3-hardhat/contracts/",
